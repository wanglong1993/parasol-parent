<?xml version="1.0" encoding="GB2312"?>
<dal>
	<default>
		<dbStrategy name=""
			clazz="com.lenovo.vctl.dal.dao.route.strategy.DefaultStrategy">
			<pattern value="[0-9]$" group="g2" />
		</dbStrategy>
		<listenerClass className="" />
		<idCenterDS ds="idCenterDS" />   <!-- 缺省IdCenter使用的数据源配置 -->
	</default>
	<route>
		<!-- 在一个侦听器中实现before after -->
		<object name="com.ginkgocap.ywxt.metadata.model.Code" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="userId">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>
			<!-- 	
			<map name="VctlUser_Map_userId_passport"
                 sqlitem="select user_id from vctl_user where passport=?"
                 keyProperty="passport" valueProperty="userId" />
			 
			 <list name="User_List_userId_gender"
                  sqlitem="select user_id from vctl_user where gender =? order by update_at desc"
                  sqlType="hsql" keyProperty="gender" valueProperty="userId"  update="false"/>
            -->
           
           <!-- 通过名字找Code --> 
           <list name="Code_List_Id_Name"
                  sqlitem="select id from tb_code where name=? order by orderNo asc, useCount desc"
                  sqlType="hsql" keyProperty="name" valueProperty="id"  update="true"/>
            
           <list name="Code_List_Id_Type_Level_UseType"
                  sqlitem="select id from tb_code where type=? and level=? and useType=? order by orderNo asc,useCount desc"
                  sqlType="hsql" keyProperty="type,level,useType" valueProperty="id"  update="true"/>
                  
           <list name="Code_List_Id_All"
                  sqlitem="select id from tb_code where 1=? order by orderNo asc, useCount desc"
                  sqlType="hsql" keyProperty="all" valueProperty="id"  update="true"/>       

           <list name="Code_List_Id_useType"
                  sqlitem="select id from tb_code where 1=? order by orderNo asc,useCount desc"
                  sqlType="hsql" keyProperty="useType" valueProperty="id"  update="true"/>
                  
           <list name="Code_List_Id_useType_Type"
                  sqlitem="select id from tb_code where useType=? and type=? "
                  sqlType="hsql" keyProperty="useType,type" valueProperty="id"  update="true"/>

           <list name="Code_List_Id_Root_useType"
                  sqlitem="select id from tb_code where root=? and useType=?"
                  sqlType="hsql" keyProperty="root,useType" valueProperty="id"  update="true"/>
		</object>
		
		<object name="com.ginkgocap.ywxt.metadata.model.CodeDy" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="ctime">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>

           <!-- 通过名字找Code --> 
                  
           <list name="CodeDy_List_Id_All"
                  sqlitem="select id from tb_code_dy where 1=?"
                  sqlType="hsql" keyProperty="all" valueProperty="id"  update="true"/>
           <map name="CodeDy_Map_Id_Name"
                 sqlitem="select id from tb_code_dy where name=?"
                 keyProperty="name" valueProperty="id" />       
		</object>
		<object name="com.ginkgocap.ywxt.metadata.model.CountryCode" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="code">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>

           <list name="CountryCode_List_Id_All"
                  sqlitem="select id from tb_country_code where 1=?"
                  sqlType="hsql" keyProperty="code" valueProperty="id"  update="true"/>
		</object>
		
		<object name="com.ginkgocap.ywxt.metadata.model.EmailSuffix" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="status">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>

           <list name="EmailSuffix_List_Id_Status"
                  sqlitem="select id from tb_email_suffix where status = ? ORDER BY  id"
                  sqlType="hsql" keyProperty="status" valueProperty="id"  update="true"/>
		</object>

		<object name="com.ginkgocap.ywxt.metadata.model.CodeRegion" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="parentId">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>

           <list name="CodeRegion_List_Id_ParentId"
                  sqlitem="select id from tb_code_region where parentId=? ORDER BY orders asc"
                  sqlType="hsql" keyProperty="parentId" valueProperty="id"  update="true"/>
           <list name="CodeRegion_List_Id_All"
                  sqlitem="select id from tb_code_region where 1=? ORDER BY orders asc"
                  sqlType="hsql" keyProperty="all" valueProperty="id"  update="true"/>
		</object>
				
		<object name="com.ginkgocap.ywxt.metadata.model.SensitiveWord" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="type">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>

           <list name="SensitiveWord_List_Id_All"
                  sqlitem="select id from tb_sensitive_word where 1=?"
                  sqlType="hsql" keyProperty="all" valueProperty="id"  update="true"/>
		</object>
		
		<object name="com.ginkgocap.ywxt.metadata.model.Suggestion" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="uid">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>

           <list name="Suggestion_List_Id_All"
                  sqlitem="select id from tb_suggestion where 1=?"
                  sqlType="hsql" keyProperty="all" valueProperty="id"  update="true"/>
		</object>
		<object name="com.ginkgocap.ywxt.metadata.model.Dic" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="type">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>

           <list name="Dic_List_Id_Type"
                  sqlitem="select id from tb_dic where type=?"
                  sqlType="hsql" keyProperty="type" valueProperty="id"  update="true"/>
		</object>
		
		<object name="com.ginkgocap.ywxt.metadata.model.UserTag" listenerClass="com.ginkgocap.ywxt.framework.dal.dao.listener.DefaultListener" strategyProperty="userId">
			<dbStrategy name="" clasz="com.ginkgocap.ywxt.framework.dal.dao.route.strategy.DefaultStrategy" useCRC32="true">
				<pattern value="[02468]$" group="g2" />
				<pattern value="[13579]$" group="g3" />
			</dbStrategy>

           <list name="UserTag_List_Id_UserId"
                  sqlitem="select tagId from tb_user_tags where userId=?"
                  sqlType="hsql" keyProperty="userId" valueProperty="tagId"  update="true"/>
                  
           <list name="UserTag_List_Id_UserId_TagName"
                  sqlitem="select tagId from tb_user_tags where userId=? and tagName=?"
                  sqlType="hsql" keyProperty="userId,tagName" valueProperty="tagId"  update="true"/>
		</object>
	</route>
</dal>

