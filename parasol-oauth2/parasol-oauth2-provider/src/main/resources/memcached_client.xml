              <keyPattern value="[0-9A-Za-z]$" datasource="default" />
          </region>
		<!-- Object config -->
 		<region name="com.ginkgocap.parasol.oauth2.model.OauthClientDetails"  listenerClass="com.ginkgocap.ywxt.framework.dal.cache.listener.DefaultListenerImpl" strategyClass="com.ginkgocap.ywxt.framework.dal.cache.memcached.route.strategy.DefaultStrategy" localCache="true" >
            <keyPattern value="[02468]$" datasource="object1" />
            <keyPattern value="[13579]$" datasource="object2" />
        </region>
