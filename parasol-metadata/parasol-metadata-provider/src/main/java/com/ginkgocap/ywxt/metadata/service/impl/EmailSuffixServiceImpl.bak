package com.ginkgocap.ywxt.metadata.service.impl;

import java.util.List;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import com.ginkgocap.ywxt.metadata.dao.EmailSuffixDao;
import com.ginkgocap.ywxt.metadata.model.EmailSuffix;
import com.ginkgocap.ywxt.metadata.service.EmailSuffixService;
/**
 * 邮箱后缀service实现类
 * <p>  </p>
 * @author liubang
 * @date 2015-1-15
 */
@Service("emailSuffixService")
public class EmailSuffixServiceImpl implements EmailSuffixService {
	@Resource
	EmailSuffixDao emailSuffixDao;
	@Override
	public EmailSuffix findOne(long id) {
		return emailSuffixDao.findOne(id);
	}

	@Override
	public List<String> findAll() {
		return emailSuffixDao.findAll();
	}

	@Override
	public EmailSuffix saveOrUpdate(EmailSuffix emailSuffix) {
		return emailSuffixDao.saveOrUpdate(emailSuffix);
	}

	@Override
	public void delete(long id) {
		emailSuffixDao.delete(id);
	}

}
