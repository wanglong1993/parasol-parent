package com.ginkgocap.ywxt.metadata.dao.suggestion;

import java.util.List;

import com.ginkgocap.ywxt.metadata.model.Dic;
import com.ginkgocap.ywxt.metadata.model.Suggestion;
/**
 * 类型Dao接口
 * @author LK
 * 创建时间：2013-03-14 15:20:05
 */
public interface SuggestionDao {
    /**
     * 通过主键获得反馈记录
     * @param id
     * @return
     */
    Suggestion selectByPrimarKey(long id);
    /**
     * 插入反馈记录
     * @param suggestion
     * @return
     */
    Suggestion insert(Suggestion suggestion);
    Suggestion add(Suggestion suggestion);
    /**
     * 修改反馈记录
     * @param suggestion
     */
    void update(Suggestion suggestion);
    /**
     * 删除反馈记录
     * @param id
     */
    void delete(long id);
    /**
     * 根据条件查询
     * @param id 反馈编号
     * @param user_name 反馈人登录名
     * @param problemTitle反馈标题
     * @param feedbackType反馈类型
     * @param cstart搜索-反馈开始时间
     * @param cend搜索-反馈结束时间
     * @param fstart搜索-处理时间开始
     * @param fend搜索-处理时间结束
     * @param startRow 分页开始行
     * @param pageSize 行大小
     * @return 查询结果 size=0 if no result
     */
    List<Suggestion> selectByParams(long id,String user_name,String problemTitle,String cstart,String cend,String fstart,String fend,String feedbackType,Integer start,Integer size);
    List<Dic> selectDics();
    Long selectByParamsCount(long id,String user_name,String problemTitle,String cstart,String cend,String fstart,String fend,String feedbackType);
}
