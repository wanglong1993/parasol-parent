package com.ginkgocap.ywxt.metadata.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.ginkgocap.ywxt.metadata.dao.CodeDyDAO;
import com.ginkgocap.ywxt.metadata.model.CodeDy;
import com.ginkgocap.ywxt.metadata.service.CodeDyService;


/**
 * 地域的service实现类
 * @author liu
 * @创建时间：2011-11-29 17:19:31
 */
@Service("codeDyService")
public class CodeDyServiceImpl implements CodeDyService {
    @Autowired
    private CodeDyDAO codeDyDAO;

    /*
     * (non-Javadoc)
     * 查询所有的地域
     * @see com.ginkgocap.ywxt.service.CodeDyService#selectAll()
     */
    public List<CodeDy> selectAll() {
        List<CodeDy> list = codeDyDAO.selectAll();

        return list;
    }

    /*
     * (non-Javadoc)
     * 通过id查询地域
     * @see com.ginkgocap.ywxt.service.CodeDyService#selectByPrimarKey(long)
     */
    public CodeDy selectByPrimarKey(long id) {
        CodeDy codeDy = codeDyDAO.selectByPrimarKey(id);
        if (null == codeDy) {
            codeDy = new CodeDy();
            codeDy.setId(0);
        }
        return codeDy;
    }

    /*
     * (non-Javadoc)
     * 插入地域
     * @see com.ginkgocap.ywxt.service.CodeDyService#insert(com.ginkgocap.ywxt.model.CodeDy)
     */
    public CodeDy insert(CodeDy codeDy) {
        CodeDy codeDy1 = codeDyDAO.insert(codeDy);
        return codeDy1;
    }

    /*
     * 修改地域
     * (non-Javadoc)
     * @see com.ginkgocap.ywxt.service.CodeDyService#update(com.ginkgocap.ywxt.model.CodeDy)
     */
    public void update(CodeDy codeDy) {
        codeDyDAO.update(codeDy);

    }

    /*
     * (non-Javadoc)
     * 通过名字查询地域
     * @see com.ginkgocap.ywxt.service.CodeDyService#selectByName(java.lang.String)
     */
    public CodeDy selectByName(String name) {
        CodeDy codeDy = codeDyDAO.selectByName(name);
        return codeDy;
    }

    /*
     * 删除地域
     * (non-Javadoc)
     * @see com.ginkgocap.ywxt.service.CodeDyService#delete(long)
     */
    public void delete(long id) {
        codeDyDAO.delete(id);

    }

}
